{
    "presets": [
        "@babel/preset-env",
        [
            "@babel/preset-react",
            {
                "runtime": "automatic"
            }
        ],
        "@babel/preset-typescript"
    ],
    "env": {
      "development": {
        "plugins": ["babel-plugin-styled-components"]
      }
    },
    "plugins": [
      ["@babel/plugin-transform-runtime",
      {
        "regenerator": true
      }]
    ]
}